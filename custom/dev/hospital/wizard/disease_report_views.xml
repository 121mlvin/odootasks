<?xml version="1.0" encoding="utf-8" ?>

<odoo>

    <record id="action_disease_report" model="ir.actions.act_window">
        <field name="name">Disease Report</field>
        <field name="res_model">disease.report</field>
        <field name="view_mode">tree</field>
        <field name="target">new</field>
    </record>


    <record id="view_disease_report_tree" model="ir.ui.view">
        <field name="name">disease.report.tree</field>
        <field name="model">disease.report</field>
        <field name="arch" type="xml">
            <tree string="Disease Report">
                <field name="disease_name"/>
                <field name="diagnosis_count"/>
            </tree>
        </field>
    </record>
</odoo>
